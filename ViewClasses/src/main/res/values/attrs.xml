<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆角ImageView-->
    <declare-styleable name="RoundImageView">
        <attr name="image_radis" format="dimension" />
        <attr name="image_angle" format="dimension" />
    </declare-styleable>

    <declare-styleable name="rainbowbar">
        <attr name="rainbowbar_hspace" format="dimension"/>
        <attr name="rainbowbar_vspace" format="dimension"/>
        <attr name="rainbowbar_color" format="color"/>
    </declare-styleable>

    <!--数字SeekBar  NumberSeekBar-->
    <declare-styleable name="NumberSeekBar">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textBackground" format="reference" />
        <attr name="textOrientation" format="enum">
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>
</resources>